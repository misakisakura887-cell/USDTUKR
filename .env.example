# Environment Variables for USDTUKR ETF Platform

# WalletConnect Project ID (required for wallet connections)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Alchemy API Keys for different networks (optional, for enhanced RPC)
NEXT_PUBLIC_ALCHEMY_API_KEY_MAINNET=your_alchemy_mainnet_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_POLYGON=your_alchemy_polygon_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_ARBITRUM=your_alchemy_arbitrum_key_here

# Infura Project ID (alternative RPC provider)
NEXT_PUBLIC_INFURA_PROJECT_ID=your_infura_project_id_here

# Contract Addresses for UKR Token on different networks
NEXT_PUBLIC_UKR_CONTRACT_MAINNET=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_UKR_CONTRACT_POLYGON=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_UKR_CONTRACT_BSC=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_UKR_CONTRACT_ARBITRUM=0x1234567890123456789012345678901234567890

# DEX Router Addresses for trading
NEXT_PUBLIC_UNISWAP_V3_ROUTER=0xE592427A0AEce92De3Edee1F18E0157C05861564
NEXT_PUBLIC_PANCAKESWAP_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E
NEXT_PUBLIC_QUICKSWAP_ROUTER=0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff

# API Keys for price feeds (optional)
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_api_key_here
NEXT_PUBLIC_COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Analytics and Monitoring (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_tracking_id_here
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token_here

# Development/Production Environment
NODE_ENV=development
NEXT_PUBLIC_ENVIRONMENT=development